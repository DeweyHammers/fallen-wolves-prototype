<div>
  <h1><%= @post.title %></h1>
  <p><%= @post.content %></p>
  <% if current_user.id == @post.member.id %>
    <p><%= link_to 'Edit', edit_post_path(@post)  %>
    <%= link_to 'Delete', @post , method: :delete %></p>
  <% end %>
  <%= render partial: '/members/partails/member_info', locals: { member: @post.member, style: 'height:50px;width:50px;'} %>
</div>
